{
  "name": "profile",
  "version": "1.0.0",
  "description": "Landing page for out-door",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.production.config.js",
    "start": "webpack-dashboard -- webpack-dev-server --config webpack.dev.config.js --hot --open",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build-old:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Pinchas",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack-dashboard": "^3.0.5",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.1",
    "chart.js": "^2.7.3",
    "uniqid": "^5.0.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
